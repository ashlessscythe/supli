{
  "name": "office-supplies-tracker",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "server:dev": "ts-node-dev --respawn --transpile-only server/index.ts",
    "server:build": "tsc -p server/tsconfig.json",
    "server:start": "node dist/server/index.js",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,jsx,tsx,astro}\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,astro,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,jsx,tsx,astro,css,md,json}\""
  },
  "dependencies": {
    "@astrojs/solid-js": "^3.0.2",
    "@prisma/client": "^5.6.0",
    "astro": "^3.6.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "solid-js": "^1.8.5"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^5.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.1",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}